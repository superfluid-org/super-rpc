# Core server config
PORT=3000
HOST=0.0.0.0
NODE_ENV=development
LOG_LEVEL=info

# Upstream RPC (required to run)
# RPC_URL=
RPC_TIMEOUT=30000
RPC_RETRIES=10
RPC_INITIAL_TIMEOUT=2000

# Cache
CACHE_MAX_AGE=10
CACHE_MAX_SIZE=10000
ENABLE_DB_CACHE=false
# DB_FILE=./cache.sqlite

# Rate limiting (reserved for future use)
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=1000

# Security
ALLOWED_ORIGINS=
DISABLE_HELMET=false
DISABLE_CORS=false
